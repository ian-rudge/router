{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rulesDirectory": [
    "tslint-eslint-rules"
  ],
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "no-increment-decrement": false,
    "import-name": false,
    "ter-padded-blocks": [
      true,
      "never"
    ],
    "align": [true, "parameters", "statements", "members", "elements"]
  }
}
