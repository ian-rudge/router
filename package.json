{
  "name": "@servall/router",
  "version": "1.1.5",
  "description": "Common koa routing for Servall",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Servall Development",
    "email": "admin@servalldev.com",
    "url": "https://www.servalldev.com"
  },
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.servalldatasystems.com"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "yarn build",
    "lint": "dev lint",
    "fix": "dev lint --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "8",
    "@servall/commitlint": "1",
    "@servall/dev": "0.0.10-beta.13",
    "@servall/eslint-config": "0.1",
    "@servall/jest": "0.1",
    "@servall/prettier": "0.1",
    "@servall/router": "link:./.",
    "@servall/router-testing": "1",
    "@servall/tsconfig": "0.1",
    "@types/fs-extra": "8",
    "@types/jest": "24",
    "@types/js-yaml": "3",
    "@types/lodash": "4",
    "husky": "3",
    "jest": "24",
    "tmp-promise": "1",
    "typescript": "3"
  },
  "dependencies": {
    "@serafin/open-api": "3",
    "@serafin/schema-builder": "0.9",
    "@servall/mapper": "0.1",
    "@servall/ts": "0.2",
    "@types/koa": "2",
    "@types/koa-bodyparser": "4",
    "@types/koa-router": "7",
    "@types/yup": "0.26",
    "ajv": "6",
    "fs-extra": "8",
    "js-yaml": "3",
    "koa": "2",
    "koa-bodyparser": "4",
    "koa-router": "7",
    "lodash": "4",
    "resolve-from": "5",
    "stacktrace-parser": "0.1",
    "yup": "0.26"
  },
  "prettier": "@servall/prettier",
  "eslintConfig": {
    "extends": "@servall"
  },
  "jest": {
    "preset": "@servall/jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@servall/commitlint"
    ]
  }
}
